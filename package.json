{
	"name": "tkww-assistant",
	"displayName": "TKWW Assistant",
	"description": "The Knot Worldwide Assistant VSCode Extension",
	"version": "0.0.1",
	"publisher": "TKWW-Engineering-Team",
	"repository": {
		"type": "git",
		"url": "https://github.com/cameronaziz/tkww-assistant"
	},
	"engines": {
		"vscode": "^1.50.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:tkww.convertFile"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "tkww.convertFile",
				"title": "Convert to TKWW Union CSS Variables"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.50.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^12.11.7",
		"eslint": "^7.9.0",
		"@typescript-eslint/eslint-plugin": "^4.1.1",
		"@typescript-eslint/parser": "^4.1.1",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"typescript": "^4.0.2",
		"vscode-test": "^1.4.0"
	}
}
